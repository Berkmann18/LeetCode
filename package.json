{
  "name": "leetcode",
  "version": "0.0.0",
  "description": "My LeetCode solutions",
  "main": "index.js",
  "scripts": {
    "start:ts": "ts-node",
    "start": "node",
    "test": "jest",
    "cc": "cr -c ./.complexrc",
    "cc:all": "npm run cc *.js src/*.js",
    "lint": "eslint -c ./.eslintrc.json **/*.{js,ts} --fix",
    "fmt": "prettier --config .prettierrc.json --ignore-path .prettierignore --write",
    "build": "tsc --build tsconfig.json",
    "build:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Berkmann18@github.com/Berkmann18/LeetCode.git"
  },
  "keywords": [
    "LeetCode",
    "solutions"
  ],
  "author": {
    "name": "Maximilian Berkmann",
    "email": "maxieberkmann@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Berkmann18/LeetCode/issues"
  },
  "homepage": "https://github.com/Berkmann18/LeetCode#readme",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.4",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "complexity-report": "^2.0.0-alpha",
    "cz-adapter-eslint": "^0.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jquery": "^1.5.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.5.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "lint-staged": {
    "*.(js|ts)": [
      "npm run fmt",
      "eslint -c ./.eslintrc.json --fix",
      "git add"
    ],
    "*.py": [
      "pylint",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 5.6.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-adapter-eslint"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.js",
      "src/*.js",
      "test/*.js",
      "!**/node_modules/**",
      "!**/_*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 50,
        "statements": 50
      }
    },
    "testEnvironment": "node"
  }
}
